# RentCars Frontend Environment Configuration
# Copy this file to .env.local and fill in your values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SOCIAL_BOOKING=true
REACT_APP_ENABLE_LOYALTY_PROGRAM=true

# App Configuration
REACT_APP_NAME=RentCars
REACT_APP_VERSION=2.0.0

# Google Maps API (optional - for trip planner)
REACT_APP_GOOGLE_MAPS_KEY=your_google_maps_api_key_here

# Stripe Public Key (for payments)
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here
